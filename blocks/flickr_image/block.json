{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 3,
	"name": "ptws/flickr-image",
	"version": "0.3.6",
	"title": "PTWS: Flickr Image",
	"category": "text",
	"icon": "index-card",
	"description": "One image with metadata fetched from Flickr.",
	"textdomain": "ptws",
	"attributes": {
		"flickr_id": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.flickrid"
		},
		"flickr_id_is_valid": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.flickridisvalid"
		},
		"cached_time": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.cachedtime"
		},
		"cached_time_epoch": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.cachedtimeepoch"
		},
		"description": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.description"
		},
		"height": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.height"
		},
		"id": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.id"
		},
		"large_thumbnail_height": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.largethumbnailheight"
		},
		"large_thumbnail_url": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.largethumbnailurl"
		},
		"large_thumbnail_width": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.largethumbnailwidth"
		},
		"link_url": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.linkurl"
		},
		"square_thumbnail_height": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.squarethumbnailheight"
		},
		"square_thumbnail_url": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.squarethumbnailurl"
		},
		"square_thumbnail_width": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.squarethumbnailwidth"
		},
		"taken_time": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.takentime"
		},
		"taken_time_epoch": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.takentimeepoch"
		},
		"title": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.title"
		},
		"updated_time": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.updatedtime"
		},
		"updated_time_epoch": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.updatedtimeepoch"
		},
		"uploaded_time": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.uploadedtime"
		},
		"uploaded_time_epoch": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.uploadedtimeepoch"
		},
		"width": {
			"type": "string",
			"source": "text",
			"selector": "div.photodata > div.width"
		}
	},
	"example": {
		"attributes": {
			"flickr_id": "0000000",
			"flickr_id_is_valid": true,
			"taken_time_epoch": "2024-12-12 00:43:44",
			"uploaded_time_epoch": "2024-12-12 00:43:44",
			"updated_time_epoch": "2024-12-12 00:43:44",
			"cached_time_epoch": "2024-12-12 00:43:44"
		}
	},
	"editorScript": "file:./block.js",
	"style": "file:./style.css"
}