{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ptws/itinerary",
	"title": "PTWS: Itinerary",
	"icon": "index-card",
	"category": "layout",
	"attributes": {
		"from": {
			"type": "string",
			"source": "html",
			"selector": "h4.from"
		},
		"to": {
			"type": "string",
			"source": "html",
			"selector": "h4.to"
		},
		"mediaID": {
			"type": "number"
		},
		"mediaURL": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		},
		"steps": {
			"type": "string",
			"source": "html",
			"selector": ".steps"
		}
	},
	"example": {
		"attributes": {
			"from": "Whakatane, New Zealand",
			"to": "Rotorua Museum, Government Gardens, Queens Drive, Rotorua 3046",
			"mediaURL": "https://mile42.net/pics-maps/nz/Map-Day_14-a.png",
			"steps": ["turn left at the fart", "now face west", "think about direction", "oops you should have turned right"]
		}
	},
	"editorScript": "file:./block.js",
	"style": "file:./style.css"
}